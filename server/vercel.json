{
    "version": 2,
    "builds": [
      {
        "src": "server/api/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/api/index.js"
      }
    ]
  }
  